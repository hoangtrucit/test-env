{
  "name": "om-apm-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "build:local": "cross-env NODE_ENV=local nest build",
    "build:dev": "cross-env NODE_ENV=development nest build",
    "build:test": "cross-env NODE_ENV=test nest build",
    "build:stg": "cross-env NODE_ENV=staging nest build",
    "build:prod": "cross-env NODE_ENV=production nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint --ext .ts,.tsx '{src,test}/**/*.ts' --fix --no-error-on-unmatched-pattern",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:tsc": "tsc --project ./tsconfig.json",
    "lint:report": "eslint \"{src,apps,libs,test}/**/*.ts\" -f ./scripts/eslint_export.js -o ./scripts/eslint_report.json",
    "test": "jest --config ./test/jest.json --runInBand --detectOpenHandles --no-cache",
    "test:watch": "yarn test --watchAll",
    "test:e2e:watch": "yarn test:e2e --watchAll",
    "test:cov": "jest --config ./test/jest.json --runInBand --coverage --detectOpenHandles --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --detectOpenHandles --no-cache",
    "test:clear": "jest --clearCache",
    "postinstall": "[[ -d './.husky' ]] && exit 0; husky install;"
  },
  "dependencies": {
    "@apollo/server": "^4.7.3",
    "@nestjs/apollo": "^11.0.6",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^11.0.6",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/typeorm": "^9.0.1",
    "@ocean-network-express/om-apm-share": "^0.5.0",
    "@types/lodash": "^4.14.195",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.6.0",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "nestjs-typeorm-paginate": "^4.0.3",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitizen": "^4.3.0",
    "commitlint-plugin-function-rules": "^2.0.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "testcontainers": "^9.9.0",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
